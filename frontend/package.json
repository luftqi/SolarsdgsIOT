{
  "name": "solarsdgs-iot-frontend",
  "version": "1.0.0",
  "description": "SolarSDGs IoT Frontend - Vue 3 + TypeScript + Vite",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "lint": "eslint . --ext .vue,.ts,.tsx",
    "lint:fix": "eslint . --ext .vue,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{vue,ts,tsx,css}\""
  },
  "dependencies": {
    "vue": "^3.4.3",
    "vue-router": "^4.2.5",
    "pinia": "^2.1.7",
    "axios": "^1.6.5",
    "socket.io-client": "^4.6.2",
    "chart.js": "^4.4.1",
    "vue-chartjs": "^5.3.0",
    "leaflet": "^1.9.4",
    "@vueuse/core": "^10.7.1",
    "dayjs": "^1.11.10"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.2",
    "vite": "^5.0.10",
    "vue-tsc": "^1.8.27",
    "typescript": "^5.3.3",
    "@types/node": "^20.10.5",
    "@types/leaflet": "^1.9.8",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.2",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "prettier": "^3.1.1",
    "vitest": "^1.1.0",
    "@vitest/ui": "^1.1.0",
    "@playwright/test": "^1.40.1",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
