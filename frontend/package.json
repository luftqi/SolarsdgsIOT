{
  "name": "solarsdgs-iot-frontend",
  "version": "1.0.0",
  "description": "SolarSDGs IoT Frontend - Vue 3 + TypeScript + Vite",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "lint": "eslint . --ext .vue,.ts,.tsx",
    "lint:fix": "eslint . --ext .vue,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{vue,ts,tsx,css}\""
  },
  "dependencies": {
    "@vueuse/core": "^10.7.1",
    "axios": "^1.6.5",
    "chart.js": "^4.4.1",
    "chartjs-adapter-dayjs-4": "^1.0.4",
    "chartjs-plugin-annotation": "^3.1.0",
    "chartjs-plugin-zoom": "^2.2.0",
    "dayjs": "^1.11.10",
    "file-saver": "^2.0.5",
    "leaflet": "^1.9.4",
    "papaparse": "^5.5.3",
    "pinia": "^2.1.7",
    "socket.io-client": "^4.6.2",
    "v-viewer": "^3.0.22",
    "viewerjs": "^1.11.7",
    "vue": "^3.4.3",
    "vue-chartjs": "^5.3.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@types/file-saver": "^2.0.7",
    "@types/leaflet": "^1.9.8",
    "@types/node": "^20.10.5",
    "@types/papaparse": "^5.5.0",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "@vitejs/plugin-vue": "^5.0.2",
    "@vitest/ui": "^1.6.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.2",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3",
    "vite": "^6.4.1",
    "vitest": "^1.6.1",
    "vue-tsc": "^2.2.12"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
